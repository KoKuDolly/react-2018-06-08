{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": ["error", "never"],
    "linebreak-style": "off",
    "semi": "off",
    "space-before-blocks": "off",
    "comma-spacing": "off",
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "react/no-array-index-key": 0,
    "arrow-body-style": 0,
    "react/prefer-stateless-function": 0,
    "no-console": 0
  },
  "globals": {
    "document": true,
    "localStorage": true,
    "window": true
  }
}